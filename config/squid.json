{
  "default": {
    "debug": false,
    "github": {
      "baseurl":         "https://api.github.com",
      "headers":  {
        "Accept":        "application/vnd.github.v3+json",
        "Content-Type":  "application/json;charset=UTF-8"
      },
      "pagination": {
        "repositories":   50
      },
      "authorization":    "token"
    },
    "updater": {
      "source": {
          "host": "getsquiddone.com",
          "path": "/download/squid-lastest.dmg"
      },
      "version": "/VERSION"
    },
    "window": {
      "title": "Squid",
      "toolbar": false,
      "width": 380,
      "height": 465,
      "resizable": false,
      "frame": false,
      "transparent": true,
      "show": false
    }
  }
}
